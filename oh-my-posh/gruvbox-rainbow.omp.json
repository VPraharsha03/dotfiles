{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "color_fg0": "#fbf1c7",
    "color_bg1": "#3c3836",
    "color_bg3": "#665c54",
    "color_blue": "#458588",
    "color_aqua": "#689d6a",
    "color_green": "#98971a",
    "color_orange": "#d65d0e",
    "color_purple": "#b16286",
    "color_red": "#cc241d",
    "color_yellow": "#d79921"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "foreground": "p:color_fg0",
          "background": "p:color_orange",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} {{ .UserName }} ",
          "properties": {
            "windows" : "Û∞ç≤",
            "macos": "mac"
          }
        },
        {
          "background": "p:color_yellow",
          "foreground": "p:color_fg0",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "full",
            "mapped_locations": {
                "{{.Env.USERPROFILE}}\\Documents" : "Û∞àô ",
                "{{.Env.USERPROFILE}}\\Downloads" : "ÔÄô ",
                "{{.Env.USERPROFILE}}\\Music" : "Û∞ùö ",
                "{{.Env.USERPROFILE}}\\Pictures" : "ÔÄæ ",
                "{{.Env.USERPROFILE}}\\Developer" : "Û∞≤ã "
            }
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:color_aqua",
          "foreground": "p:color_fg0",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true,
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "style": "accordion",
          "template": " \u279c ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}) ",
          "type": "git"
        },
        {
          "background": "p:color_blue",
          "foreground": "p:color_fg0",
          "powerline_symbol": "\ue0b0",
          "style": "accordion",
          "properties": {
            "home_enabled": true
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}v{{ .Full }} ",
          "type": "node"
        },
        {
          "type": "rust",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "p:color_fg0",
          "background": "p:color_blue",
          "template": "Óû® v{{ .Full }} "
        },
        {
          "type": "go",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "p:color_fg0",
          "background": "p:color_blue",
          "template": "‚Ä≠\ue627 v{{ .Full }} "
        },
        {
          "type": "php",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "p:color_fg0",
          "background": "p:color_blue",
          "template": "Óòà v{{ .Full }} "
        },
        {
          "type": "java",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "p:color_fg0",
          "background": "p:color_blue",
          "template": "Óâñ v{{ .Full }} "
        },
        {
          "type": "kotlin",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "p:color_fg0",
          "background": "p:color_blue",
          "template": "Óò¥ v{{ .Full }} "
        },
        {
          "type": "haskell",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "p:color_fg0",
          "background": "p:color_blue",
          "template": "Óòü v{{ .Full }} "
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "p:color_fg0",
          "background": "p:color_blue",
          "properties": {
            "fetch_virtual_env": true,
            "home_enabled": true
          },
          "template": "Óàµ v{{ .Full }}{{ if .Venv }} ({{ .Venv }}){{ end }} "
        },
        {
          "type": "docker",
          "style": "accordion",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#83a598",
          "background": "p:color_bg3",
          "template": " Ôåà {{ .Context }} "
        },
        {
          "type": "project",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "p:color_fg0",
          "background": "p:color_bg3",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}üì¶ {{ if .Type }}{{ .Type }}:{{ end }} <p:color_orange>v{{.Version}}</>{{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }} "
        },
        {
          "type": "executiontime",
          "style": "plain",
          "template": " took <#fefefe>Û±é´</> {{ .FormattedMs }} ",
          "properties": {
            "threshold": 0,
            "style": "austin",
            "always_enabled": true
        }
}
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:color_bg1",
          "foreground": "p:color_fg0",
          "properties": {
            "time_format": "15:04"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "style": "diamond",
          "template": " Ôê∫ {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:color_fg0",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "Ôê≤",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
